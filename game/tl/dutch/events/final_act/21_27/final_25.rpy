# game/events/final_act/21_27/final_act_25.rpy:570


translate dutch final_act_25_secret_final_b0039de5:




    # "[gg]" "Это проделки моего брата, [officer_name]!"

    "[gg]" '''Dit is het werk van mijn broer, [officer_name]!'''




# game/events/final_act/21_27/final_act_25.rpy:573


translate dutch final_act_25_secret_final_522f54be:




    # "[gg]" "И девушка, которая мне якобы помогала…"

    "[gg]" '''En het meisje dat me probeerde te helpen...'''




# game/events/final_act/21_27/final_act_25.rpy:589


translate dutch final_act_25_secret_final_2623251e:




