translate eng christie_root_39_b07c5442:

    # "[gg]" "Тот раз тебе пришлось худо."
    "[gg]" "You got it rough last time."

# game/events/christie_roots/christie_root_39.rpy:123
translate eng christie_root_39_fc8e91a0:

