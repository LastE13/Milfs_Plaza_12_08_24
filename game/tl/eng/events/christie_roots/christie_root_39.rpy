# game/events/christie_roots/christie_root_39.rpy:93
translate eng christie_root_39_c5ac6015:

    # "[gg]" "Ага."
    "[gg]" "Yeah."

# game/events/christie_roots/christie_root_39.rpy:95
translate eng christie_root_39_bbe92f51:

